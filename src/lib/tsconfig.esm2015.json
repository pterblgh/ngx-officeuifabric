{
  "compilerOptions": {
    "declaration": true,
    "module": "es2015",
    "target": "es2015",
    "baseUrl": "./tmp",
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "rootDir": ".",
    "outDir": "../esm2015",
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "importHelpers": true,
    "newLine": "lf",
    "sourceMap": true,
    "inlineSources": true,
    "lib": ["es2015", "dom"],
    "skipLibCheck": true,
    "types": []
  },
  "files": ["public-api.ts"],
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true,
    "flatModuleOutFile": "index.js",
    "flatModuleId": "ngx-officeuifabric"
  }
}
