{
    "compilerOptions": {
      "target": "es5",
      "lib": ["es6", "es2015", "dom"],
      "declaration": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "noUnusedParameters": true,
      "strictNullChecks": true,
      "skipLibCheck": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "noEmitOnError": true,
      "noImplicitAny": true,
      "sourceMap": true,
      "stripInternal": false,
      "typeRoots": [
        "../../node_modules/@types/!(node)"
      ],
      "baseUrl": ".",
      "paths": {}
    }
  }
  